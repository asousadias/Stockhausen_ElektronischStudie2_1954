;                    ; STUDIE II, Stockhausen (excerpt) Csound orchestra Version 2;; coded by Antonio de SOUSA DIAS;; in addition to version 1, this version follows Stockhausen's score instructions.; all durations are 1 second longer to allow material generation prior to listening;sr = 44100kr = 4410ksmps = 10nchnls = 1; each instrument corresponds to a different sound mixture typeinstr 1, 2, 3, 4, 5itipo	= p1	; idur	= p3iamp0 = ampdb(85-p4)iamp1 = ampdb(85-p5)ifreq0 = p6if ( itipo == 1) igoto tipo1if ( itipo == 2) igoto tipo2if ( itipo == 3) igoto tipo3if ( itipo == 4) igoto tipo4if ( itipo == 5) igoto tipo5tipo1:ifreq1	= int(ifreq0*1.0665+0.5)ifreq2	= int(ifreq0*1.1374+0.5)ifreq3	= int(ifreq0*1.2131+0.5)ifreq4	= int(ifreq0*1.2937+0.5)igoto continuetipo2:ifreq1	= int(ifreq0*1.1374+0.5)ifreq2	= int(ifreq0*1.2937+0.5)ifreq3	= int(ifreq0*1.4714+0.5)ifreq4	= int(ifreq0*1.6736+0.5)igoto continuetipo3:ifreq1	= int(ifreq0*1.2130+0.5)ifreq2	= int(ifreq0*1.4715+0.5)ifreq3	= int(ifreq0*1.7849+0.5)ifreq4	= int(ifreq0*2.1652+0.5)igoto continuetipo4:ifreq1	= int(ifreq0*1.2937+0.5)ifreq2	= int(ifreq0*1.6737+0.5)ifreq3	= int(ifreq0*2.1652+0.5)ifreq4	= int(ifreq0*2.8011+0.5)igoto continuetipo5:ifreq1	= int(ifreq0*1.3797+0.5)ifreq2	= int(ifreq0*1.9036+0.5)ifreq3	= int(ifreq0*2.6264+0.5)ifreq4	= int(ifreq0*3.6236+0.5)continue:; "kamp" represents a general amplitude envelope that will only let pass reverberated signal; "afonte" is a comparision signal used to rebalance the reverberated result.kamp	linseg	0, 0.5, 0, 0.01, iamp0, idur-0.98, iamp1, 0.01, 0, 1.0,0afonte		oscil	kamp, 10000, 1; first, we simulate the five 4 cm length tape slices editing.kamp1	linseg	0,0.0030,30000,0.0525,30000,0.003,0,idur,0a1		oscil	kamp1, ifreq0, 1kamp2	linseg	0,0.0555,30000,0.0525,30000,0.003,0,idur,0a2		oscil	kamp2, ifreq1, 1kamp3	linseg	0,0.01080,30000,0.0525,30000,0.003,0,idur,0a3		oscil	kamp3, ifreq2, 1kamp4	linseg	0,0.1605,30000,0.0525,30000,0.003,0,idur,0a4		oscil	kamp4, ifreq3, 1kamp5	linseg	0,0.2130,30000,0.0525,30000,0.003,0,idur,0a5		oscil	kamp5, ifreq4, 1; second, the previous result enters in a 10 seconds reverberation chambera0		reverb	(a1 + a2 + a3 + a4 + a5), 10; finally it is balanced with "afonte"asig		balance	a0, afonte		out	asigendin